[PrefixDeclaration]
:		http://www.semanticweb.org/spark-datatypes-test#
dc:		http://purl.org/dc/elements/1.1/
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
foaf:		http://xmlns.com/foaf/0.1/
geo:		http://www.opengis.net/ont/geosparql#
obda:		https://w3id.org/obda/vocabulary#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
skos:		http://www.w3.org/2004/02/skos/core#
terms:		http://purl.org/dc/terms/

[MappingDeclaration] @collection [[
mappingId       raster_1
target          :{origin} a :TestObject ; geo:asWKT {pointshape}^^geo:wktLiteral ; :hasRSMean {rsmean}^^xsd:double ; :hasRSMode {rsmode}^^xsd:double ; :hasRSMax {rsmax}^^xsd:double ; :hasRSMin {rsmin}^^xsd:double .
source          SELECT origin,
                        ST_GEOMFROMWKT(geom) AS pointshape,
                        RS_MEAN(data) AS rsmean,
                        RS_MODE(data) AS rsmode,
                        ARRAY_MAX(ARRAY_EXCEPT(data,ARRAY(FLOAT('NaN')))) AS rsmax,
                        ARRAY_MIN(data) AS rsmin
                FROM default.my_geotiff_table;
]]

